{
	"name": "@hexcuit/server",
	"version": "0.8.2",
	"type": "module",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/hexcuit/server"
	},
	"files": [
		"dist"
	],
	"exports": {
		".": {
			"import": "./dist/client.js",
			"types": "./dist/client.d.ts"
		}
	},
	"scripts": {
		"dev": "wrangler dev",
		"deploy": "wrangler deploy --minify",
		"build": "bun run generate:client && tsup",
		"generate:client": "bun scripts/generate-client.ts",
		"check": "biome check",
		"check:fix": "biome check --fix --unsafe",
		"cf-typegen": "wrangler types --env-interface CloudflareBindings",
		"typecheck": "tsc --noEmit",
		"generate": "drizzle-kit generate",
		"migrate:dev": "wrangler d1 migrations apply hexcuit --local",
		"test": "vitest run",
		"test:watch": "vitest",
		"test:coverage": "vitest run --coverage",
		"version": "changeset version && bun i --lockfile-only",
		"version:canary": "changeset version --snapshot canary && bun i --lockfile-only",
		"release": "changeset publish",
		"release:canary": "changeset publish --tag canary --no-git-tag"
	},
	"dependencies": {
		"@hono/zod-openapi": "1.1.5",
		"hono": "4.11.1"
	},
	"devDependencies": {
		"@biomejs/biome": "2.3.10",
		"@changesets/changelog-github": "0.5.2",
		"@changesets/cli": "2.29.8",
		"@cloudflare/vitest-pool-workers": "0.11.0",
		"@scalar/hono-api-reference": "0.9.28",
		"@types/node": "25.0.3",
		"@vitest/coverage-istanbul": "3.2.4",
		"drizzle-kit": "0.31.8",
		"drizzle-orm": "0.45.1",
		"drizzle-zod": "0.8.3",
		"lefthook": "2.0.12",
		"tsup": "8.5.1",
		"typescript": "5.9.3",
		"vitest": "3.2.4",
		"wrangler": "4.55.0",
		"zod": "4.2.1"
	}
}
