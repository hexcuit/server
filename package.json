{
	"name": "@hexcuit/server",
	"version": "0.11.1",
	"type": "module",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/hexcuit/server"
	},
	"files": [
		"dist"
	],
	"exports": {
		".": {
			"import": "./dist/client.js",
			"types": "./dist/client.d.ts"
		}
	},
	"scripts": {
		"dev": "wrangler dev",
		"build": "bun run generate:client && tsup",
		"check": "biome check",
		"check:fix": "biome check --fix --unsafe",
		"knip": "knip",
		"cf-typegen": "wrangler types --env-interface CloudflareBindings",
		"generate:client": "bun scripts/generate-client.ts",
		"typecheck": "tsc --noEmit",
		"typecheck:native": "tsgo --noEmit",
		"generate": "drizzle-kit generate",
		"migrate:dev": "wrangler d1 migrations apply hexcuit --local",
		"db:studio": "drizzle-kit studio --config drizzle.config.local.ts",
		"test": "vitest run",
		"test:watch": "vitest",
		"test:coverage": "vitest run --coverage",
		"check:routes": "bun scripts/check-routes.ts",
		"check:routes:fix": "bun scripts/check-routes.ts --fix",
		"version": "changeset version && bun i --lockfile-only",
		"version:canary": "changeset version --snapshot canary && bun i --lockfile-only",
		"release": "changeset publish",
		"release:canary": "changeset publish --tag canary --no-git-tag"
	},
	"dependencies": {
		"@hono/zod-openapi": "1.2.0",
		"hono": "4.11.3"
	},
	"devDependencies": {
		"@biomejs/biome": "2.3.11",
		"@cf-wasm/resvg": "0.3.3",
		"@cf-wasm/satori": "0.3.3",
		"@changesets/changelog-github": "0.5.2",
		"@changesets/cli": "2.29.8",
		"@hono/swagger-ui": "0.5.3",
		"@libsql/client": "0.17.0",
		"@types/better-sqlite3": "7.6.13",
		"@types/node": "25.0.3",
		"@typescript/native-preview": "7.0.0-dev.20260107.1",
		"@vitest/coverage-v8": "4.0.16",
		"better-sqlite3": "12.5.0",
		"drizzle-kit": "0.31.8",
		"drizzle-orm": "0.45.1",
		"drizzle-zod": "0.8.3",
		"knip": "5.80.0",
		"lefthook": "2.0.13",
		"tsup": "8.5.1",
		"typescript": "5.9.3",
		"vitest": "4.0.16",
		"wrangler": "4.57.0",
		"zod": "4.3.5"
	}
}
